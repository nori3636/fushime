{
    "version": 2,
    "builds": [
        {
            "src": "./*.py",
            "use": "@vercel/python"
        },
        {
            "src": "./functions/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}